{
    "name": "lrnzfrr/cake-slim-image",
    "description": "CakeSlimImage plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "cakephp/cakephp": "^3.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7.14|^6.0"
    },
    "autoload": {
        "psr-4": {
            "lrnzfrr\\CakeSlimImage\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CakeSlimImage\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}